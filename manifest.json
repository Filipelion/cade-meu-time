{
  "manifest_version": 2,
  "name": "Próximo Jogo do Maior do Nordeste",
  "version": "1.0",
  "description": "Uma extensão para exibir os próximos jogos do Sport Club do Recife.",
  "permissions": [
    "https://www.placardefutebol.com.br/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
